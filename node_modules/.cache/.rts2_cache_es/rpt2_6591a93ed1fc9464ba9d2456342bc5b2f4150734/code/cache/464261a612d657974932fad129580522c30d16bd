{"code":"import { config } from '../config';\r\nimport { reportPerf } from '../data/reportPerf';\r\nimport { roundByTwo } from '../helpers/utils';\r\nexport const logData = (measureName, metric, customProperties) => {\r\n    Object.keys(metric).forEach((key) => {\r\n        if (typeof metric[key] === 'number') {\r\n            metric[key] = roundByTwo(metric[key]);\r\n        }\r\n    });\r\n    // Sends the metric to an external tracking service\r\n    reportPerf(measureName, metric, customProperties);\r\n};\r\n/**\r\n * Dispatches the metric duration into internal logs\r\n * and the external time tracking service.\r\n */\r\nexport const logMetric = (duration, measureName, customProperties) => {\r\n    const duration2Decimal = roundByTwo(duration);\r\n    if (duration2Decimal <= config.maxTime && duration2Decimal >= 0) {\r\n        // 从内部或者外部的报告工具报告指标数据\r\n        reportPerf(measureName, duration2Decimal, customProperties);\r\n    }\r\n};\r\n//# sourceMappingURL=log.js.map","references":["/Users/wangyuan/Desktop/hiker-monitor/src/config/index.ts","/Users/wangyuan/Desktop/hiker-monitor/src/data/reportPerf.ts","/Users/wangyuan/Desktop/hiker-monitor/src/helpers/utils.ts"],"map":"{\"version\":3,\"file\":\"log.js\",\"sourceRoot\":\"\",\"sources\":[\"../../../../../src/data/log.ts\"],\"names\":[],\"mappings\":\"AAAA,OAAO,EAAE,MAAM,EAAE,MAAM,WAAW,CAAC;AACnC,OAAO,EAAE,UAAU,EAAE,MAAM,oBAAoB,CAAC;AAChD,OAAO,EAAE,UAAU,EAAE,MAAM,kBAAkB,CAAC;AAE9C,MAAM,CAAC,MAAM,OAAO,GAAG,CACrB,WAAmB,EACnB,MAAW,EACX,gBAAyB,EACnB,EAAE;IACR,MAAM,CAAC,IAAI,CAAC,MAAM,CAAC,CAAC,OAAO,CAAC,CAAC,GAAG,EAAE,EAAE;QAClC,IAAI,OAAO,MAAM,CAAC,GAAG,CAAC,KAAK,QAAQ,EAAE;YACnC,MAAM,CAAC,GAAG,CAAC,GAAG,UAAU,CAAC,MAAM,CAAC,GAAG,CAAC,CAAC,CAAC;SACvC;IACH,CAAC,CAAC,CAAC;IACH,mDAAmD;IACnD,UAAU,CAAC,WAAW,EAAE,MAAM,EAAE,gBAAgB,CAAC,CAAC;AACpD,CAAC,CAAC;AAEF;;;GAGG;AACH,MAAM,CAAC,MAAM,SAAS,GAAG,CACvB,QAAgB,EAChB,WAAmB,EACnB,gBAAyB,EACnB,EAAE;IACR,MAAM,gBAAgB,GAAG,UAAU,CAAC,QAAQ,CAAC,CAAC;IAC9C,IAAI,gBAAgB,IAAI,MAAM,CAAC,